  "scripts": {
    "start": "npm-run-all clean build:ts -p watch:*",
    "clean": "rimraf dist src/_js",
    "build": "npm-run-all clean build:ts build:11ty",
    "build:ts": "tsc --outDir src/_js",
    "build:11ty": "eleventy --quiet",
    "watch:ts": "yarn build:ts --watch",
    "watch:11ty": "eleventy --serve --quiet --watch",
    "lint": "eslint ./src/**/*.ts*",
    "test": "jest"
  },
  "dependencies": {
    "htm": "3.0.4",
    "preact": "10.4.8",
    "preact-render-to-string": "5.1.10"
  },
  "devDependencies": {
    "@11ty/eleventy": "0.11.0",
    "@types/node": "14.10.2",
    "@typescript-eslint/eslint-plugin": "4.1.1",
    "@typescript-eslint/parser": "4.1.1",
    "eslint": "7.9.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "typescript": "3.9.5"
  }
}
