{
  "name": "jameshill.dev",
  "version": "1.0.0",
  "description": "Home",
  "main": "index.js",
  "author": "jhukdev",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all clean build:pages build:entry -p watch:*",
    "clean": "rimraf dist src/_js",
    "build": "npm-run-all clean build:pages build:entry build:11ty",
    "build:pages": "webpack --config webpack.server.js",
    "build:entry": "webpack --config webpack.client.js",
    "build:11ty": "eleventy --quiet",
    "watch:pages": "yarn build:pages --watch",
    "watch:entry": "yarn build:entry --watch",
    "watch:11ty": "eleventy --serve --quiet --watch",
    "lint": "eslint ./src/**/*.ts*",
    "test": "jest"
  },
  "dependencies": {
    "preact": "10.5.2",
    "preact-render-to-string": "5.1.10"
  },
  "devDependencies": {
    "@11ty/eleventy": "0.11.0",
    "@types/node": "14.10.2",
    "@typescript-eslint/eslint-plugin": "4.1.1",
    "@typescript-eslint/parser": "4.1.1",
    "copy-webpack-plugin": "6.1.1",
    "css-loader": "4.3.0",
    "eslint": "7.9.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "glob": "7.1.6",
    "mini-css-extract-plugin": "0.11.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "sass": "1.26.11",
    "sass-loader": "10.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "8.0.4",
    "typescript": "4.0.3",
    "webpack": "4.44.2",
    "webpack-assets-manifest": "3.1.1",
    "webpack-cli": "3.3.12"
  }
}
